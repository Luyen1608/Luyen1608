.top-button:link,
.top-button:visited {
    text-transform: uppercase;
    text-decoration: none;
    padding: 15px 40px;
    display: inline-block;
    border-radius: 100px;
    transition: all .2s;
    position: absolute;
}

.top-button:hover {
    transform: translateY(-3px);
    box-shadow: 0 10px 20px rgba(0, 0, 0, 0.2);
}

.top-button:active {
    transform: translateY(-1px);
    box-shadow: 0 5px 10px rgba(0, 0, 0, 0.2);
}

.top-button-white {
    background-color: #D6124C;
    color: #D6124C;
}

.top-button::after {
        content: "";
    display: inline-block;
    height: 100%;
    width: 100%;
    border-radius: 5px;
    position: absolute;
    top: 0;
    left: 0%;
    z-index: -1;
    transition: all .4s;
}
.top-button-white:hover {
     animation: none;
}

.top-button-white::after {
	animation: moveInBottom 1.4s ease-out infinite;
    background-color: #D6124C;
}
@keyframes moveInBottom {
    0% {
        opacity: 1;
		 transform: scaleX(1) scaleY(1);
    }
    100% {
        opacity: 0;
		 transform: scaleX(1.1) scaleY(1.5);
    }
}
