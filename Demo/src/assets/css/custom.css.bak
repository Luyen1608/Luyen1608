.steps-vt > span:after,
  .steps-vt > span:before {
    content: "";
    display: block;
    width: 0px;
    height: 0px;
    position: absolute;
    top: 0;
    left: 0;
    border: solid transparent;
    border-left-color: #fff;
    border-width: 25px;
  }
  .steps-active-vt > span:after,
  .steps-active-vt > span:before {
    content: "";
    display: block;
    width: 0px;
    height: 0px;
    position: absolute;
    top: 0;
    left: 0;
    border: solid transparent;
    border-left-color: #EBF5FF ;
    border-width: 25px;
  }
.steps-vt .steps-active-vt {
  padding-left: 0;
  list-style: none;
  font-family: "Helvetica Neue",Helvetica,Arial,sans-serif;
  font-size: 12px;
  line-height: 1;
  margin: 30px auto;
  border-radius: 3px;
}

.steps-vt strong {
  font-size: 14px;
  display: block;
  line-height: 1.4;
}


.steps-vt>li {
  position: relative;
  display: block;
  /* border: 1px solid #ddd; */
  padding: 12px 50px 8px 50px; 
  width: 140px;
  height: 40px;
}

@media (min-width: 768px) {
  .steps-vt>li { float: left; }
  .steps-vt .past { color: #666; background: #ececec; }
  .steps-vt .present { color: #000; }
  .steps-vt .future { color: #777; background: #efefef; }

  .steps-vt li > span:after,
  .steps-vt li > span:before {
    content: "";
    display: block;
    width: 0px;
    height: 0px;
    position: absolute;
    top: 0;
    left: 0;
    border: solid transparent;
    border-left-color: #f0f0f0;
    border-width: 30px;
  }

  .steps-vt li > span:after {
    top: -5px;
    z-index: 1;
    border-left-color: white;
    border-width: 34px;
  }

  .steps-vt li > span:before { z-index: 2; }

  .steps-vt li.past + li > span:before { border-left-color: #ececec; }
  .steps-vt li.present + li > span:before { border-left-color: #fff; }
  .steps-vt li.future + li > span:before { border-left-color: #efefef; }

  .steps-vt li:first-child > span:after,
  .steps-vt li:first-child > span:before { display: none; }

  /* Arrows at start and end */
  .steps-vt li:first-child i,
  .steps-vt li:last-child i {
    display: block;
    position: absolute;
    top: 0;
    left: 0;
    border: solid transparent;
    border-left-color: #f5f6fa;
    border-width: 30px;
  }

  .steps-vt li:last-child i {
    left: auto;
    right: -30px;
    border-left-color: transparent;
    border-top-color: white;
    border-bottom-color: white;
  }
}
